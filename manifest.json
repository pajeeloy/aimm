{
  "manifest_version": 3,
  "name": "AIMM",
  "version": "1.0",
  "description": "There is no intelligence in matrix multiplication.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
